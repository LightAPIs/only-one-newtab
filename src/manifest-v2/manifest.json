{
  "manifest_version": 2,
  "name": "Only One NewTab",
  "version": "0.2.1",
  "description": "Only one new-tab can alive.",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "action": {
    "default_title": "Only one new-tab can alive"
  },
  "permissions": ["tabs", "contextMenus", "storage"]
}
